<?xml version="1.0" encoding="utf-8"?>
<com.mustang4w.wang.photopickproject.SquareRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp">

    <ImageView
        android:id="@+id/img_photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="?android:attr/selectableItemBackground"
        android:padding="10dp"
        android:src="@drawable/icon_photo_list_picker" />

    <ImageView
        android:id="@+id/img_delete"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignEnd="@id/img_photo"
        android:background="?android:attr/selectableItemBackground"
        android:src="@drawable/icon_photo_delete_corner" />

</com.mustang4w.wang.photopickproject.SquareRelativeLayout>
